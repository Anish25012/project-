import java.util.*;
/**
 * Write a description of class Cat here.
 *
 * @author (Anish Joshi)
 * @version (04/02/2021)
 */
public class Cat
{
    private String name;
    private String breed;
    private boolean vaccinated;


    public Cat(String name, String breed, boolean vaccinated){
        this.name = name;
        this.breed = breed;
        this.vaccinated = vaccinated;
    }
    /**
     * returns name
     */
    public String getName(){
        return name;
    }
    /**
     * returns breed
     */
    public String getBreed(){
        return breed;
    }
    /**
     * returns if they are vaccinated or not
     */
    public boolean Vaccinated(){
        return vaccinated;
        
    }
    /**
     * prints out the cat deatils of whether they are vaccinated or not
     */
    public String getDetails(){
        String result = "";
        if(vaccinated == true){
            result = (name +" is a "+breed+" (vaccinated) ");
        }
        else{
            result = (name +" is a "+breed);
        
        }
    return result;
    }
}
